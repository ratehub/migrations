{
  "name": "laravel-doctrine/migrations",
  "description": "Doctrine Migrations for Laravel",
  "license": "MIT",
  "keywords": [
    "doctrine",
    "laravel",
    "orm",
    "data mapper",
    "database",
    "migrations"
  ],
  "authors": [
    {
      "name": "Patrick Brouwers",
      "email": "patrick@maatwebsite.nl"
    }
  ],
   "repositories": [
    { 
      "type": "git",
      "url": "https://github.com/ratehub/laravel-doctrine.git"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "doctrine/migrations": "~1.1",
    "illuminate/config": "~5.1",
    "illuminate/contracts": "~5.1",
    "illuminate/console": "~5.1",
    "laravel-doctrine/orm": "dev-ratehub-laravel-doctrine"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "LaravelDoctrine\\Migrations\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  }
}
